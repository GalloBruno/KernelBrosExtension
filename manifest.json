{
   "action": {
      "default_title": "KernelBros IgBot"
   },
   "background": {
      "service_worker": "backgroundscript.js"
   },
   "content_scripts": [ {
      "all_frames": false,
      "css": [ "theme.blue.css", "contentscript.css", "jquery-ui.css"],
      "js": [
         "jquery3.js",
         "jquery-ui.js",
         "wNumb.js",
         "nouislider.js",
         "jquery.tablesorter.js",
         "jquery.tablesorter.widgets.js",
         "widget-pager.js",
         "contentscript.js"
      ],
      "matches": [
         "https://*.instagram.com/*",
         "https://*.kernelbros.com.ar/*"
      ],
      "run_at": "document_end"
   } ],
   "content_security_policy": {
      "extension_pages": "object-src 'self'; script-src 'self'; frame-src 'self' https://www.kernelbros.com.ar;"
   },
   "default_locale": "es",
   "description": "__MSG_appDesc__",
   "host_permissions": [
      "https://*.instagram.com/*",
      "https://*.kernelbros.com.ar/*"
   ],
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png",
      "48": "icon_48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "KernelBros IgBot",
   "permissions": [
      "tabs",
      "scripting",
      "storage",
      "unlimitedStorage"
   ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.3.7",
   "web_accessible_resources": [ {
      "matches": [
         "https://*.instagram.com/*",
         "https://*.kernelbros.com.ar/*"
      ],
      "resources": [ "*.js", "*.png", "*.css", "*.json", "*.html" ]
   } ]
}
